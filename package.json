{
  "name": "urajs",
  "version": "1.0.0",
  "description": "frontend framework",
  "type": "module",
  "scripts": {
    "start": "npm run clear && node ./scripts/server.js",
    "route": "node ./scripts/route.js",
    "comp": "node ./scripts/comp.js",
    "clear": "rm -rf ./out/*",
    "build": "node ./scripts/build.js",
    "config": "node ./scripts/config.js",
    "reset": ""
  },
  "bin": {
    "urastart": "node ./scripts/server.js",
    "uraroute": "node ./scripts/route.js",
    "uracomp": "node ./scripts/comp.js",
    "uraclear": "rm -rf ./out/*",
    "urabuild": "node ./scripts/build.js",
    "uraconfig": "node ./scripts/config.js"
  },
  "author": "Hrima Mohammed",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "chokidar": "^4.0.1",
    "enquirer": "^2.4.1",
    "express": "^5.1.0",
    "postcss": "^8.4.49",
    "sass": "^1.80.6",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "ws": "^8.18.1"
  }
}
