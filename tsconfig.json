{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "rootDir": "src",
    "outDir": "out",
    "baseUrl": "/",
    "jsx": "react",
    "jsxFactory": "Ura.e",
    "jsxFragmentFactory": "Ura.fr",
    "moduleResolution": "node",
    "skipLibCheck": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@/module/*": [
        "src/module/*"
      ],
      "ura": [
        "src/ura/code.tsx"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "docker",
    "out",
    "scripts",
    "ura.config.js",
    "tailwind.config.js",
    ".git",
    ".gitignore"
  ]
}