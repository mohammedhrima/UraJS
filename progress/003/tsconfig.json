{
    "compilerOptions": {
      "module": "ESNext",
      "target": "ES2020",
      "rootDir": "src",
      "outDir": "out",
      "jsx": "react",
      "jsxFactory": "Ura.element",
      "jsxFragmentFactory": "Ura.fragment",
      "moduleResolution": "node",
      "baseUrl": ".",
      "skipLibCheck": false,
      "allowJs": true,
      "resolveJsonModule": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["src/progress/*"]
  }
  